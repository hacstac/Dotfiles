Host github.com
  Hostname github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github_rsa

Host github_personal.com
  Hostname github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hacstac_rsa


# To check its working or not
  # ssh -Tv git@github.com
  # ssh -Tv git@github_personal.com
